import java.util.*;
public class CubeSquare
{
public static void main (String[]args)
   {
       System.out.println("Number" + "\t\t" + "Square" + "\t\t" + "Cube");
        for(int i=0;i<=10;i++){
            System.out.println(i + "\t\t\t\t" + (int)Math.pow(i,2)+ "\t\t\t\t" + (int)Math.pow(i,3));
       
       }
   }
}
